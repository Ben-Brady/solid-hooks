{
  "name": "@nnilky/solid-hooks",
  "private": false,
  "version": "0.8.0",
  "author": "Ben Brady",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ben-Brady/solid-hooks.git"
  },

  "scripts": {
    "build": "tsc",
    "publish": "npm publish --access=public",
    "benchmark": "microbundle"
  },

  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "default": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./index.js": {
      "default": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "solid",
    "threads",
    "utility"
  ],
  "files": [
    "LICENSE",
    "README.md",
    "./dist/*.d.ts",
    "./dist/*.js.map",
    "./dist/*.js",
    "./dist/*.jsx"
  ],
  "devDependencies": {
    "microbundle": "^0.15.1",
    "typescript": "~5.8.3"
  },
  "peerDependencies": {
    "solid-js": ">1.9.0"
  }
}
